{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/fl",
      "destination": "/florida",
      "permanent": true
    },
    {
      "source": "/wa",
      "destination": "/washington",
      "permanent": true
    },
    {
      "source": "/start",
      "destination": "/get-started",
      "permanent": true
    },
    {
      "source": "/assessment",
      "destination": "/get-started",
      "permanent": true
    }
  ]
}
